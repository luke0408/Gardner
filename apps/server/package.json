{
  "private": true,
  "name": "@luke0408/gardner-api",
  "version": "0.1.0",
  "description": "Starter kit of Nestia",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:webpack": "jest --watch",
    "------------------------BUILDS------------------------": "",
    "build": "npm run build:sdk && npm run build:main && npm run build:test",
    "build:api": "rimraf packages/api/lib && nestia all && rimraf packages/api/lib && tsc -p packages/api/tsconfig.json && rollup -c packages/api/rollup.config.js",
    "build:main": "rimraf lib && tsc",
    "build:sdk": "rimraf src/api/functional && nestia sdk",
    "build:swagger": "npx nestia swagger",
    "build:test": "rimraf bin && tsc -p test/tsconfig.json",
    "dev": "npm run build:test -- --watch",
    "eslint": "eslint src && eslint test",
    "eslint:fix": "eslint --fix src && eslint --fix test",
    "prepare": "ts-patch install && ts-node build/env.ts",
    "prettier": "prettier src --write && prettier test --write",
    "------------------------WEBPACK------------------------": "",
    "webpack": "rimraf dist && webpack",
    "webpack:start": "cd dist && node dist/server",
    "webpack:test": "npm run webpack && node bin/test/webpack.js",
    "------------------------DEPLOYS------------------------": "",
    "package:api": "npm run build:api && cd packages/api && npm publish",
    "start": "node lib/executable/server",
    "start:dev": "nest start --watch",
    "start:swagger": "ts-node src/executable/swagger.ts",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luke0408/Gardner"
  },
  "keywords": [
    "nestia",
    "template",
    "boilerplate"
  ],
  "author": "AUTHOR",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luke0408/Gardner/issues"
  },
  "homepage": "https://github.com/luke0408/Gardner#readme",
  "devDependencies": {
    "@autobe/interface": "^0.10.6",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.3",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@nestia/benchmark": "^8.0.5",
    "@nestia/e2e": "^8.0.5",
    "@nestia/sdk": "^8.0.5",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.6",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cli": "^0.11.21",
    "@types/cli-progress": "^3.11.5",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^8.2.5",
    "@types/node": "^18.19.121",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "chalk": "^4.1.2",
    "cli": "^1.0.1",
    "cli-progress": "^3.12.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "express": "^4.18.2",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "nestia": "^8.0.5",
    "prettier": "^3.2.4",
    "prettier-plugin-prisma": "^5.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^4.18.0",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.39.0",
    "typescript-transform-paths": "^3.5.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@apollo/server": "^4.11.3",
    "@nestia/core": "^8.0.5",
    "@nestia/fetcher": "^8.0.5",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@types/compression": "^1.8.1",
    "axios": "^1.11.0",
    "cache-manager": "^7.1.1",
    "commander": "10.0.0",
    "compression": "^1.8.1",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "inquirer": "8.2.5",
    "keyv": "^5.5.0",
    "rxjs": "^7.8.2",
    "serialize-error": "^4.1.0",
    "tgrid": "^1.1.0",
    "tstl": "^3.0.0",
    "typia": "^9.7.2",
    "uuid": "^9.0.0"
  },
  "stackblitz": {
    "startCommand": "npm run prepare && npm run build:test && npm run test -- --simultaneous 1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
