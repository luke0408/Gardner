{
  "name": "luke-test",
  "version": "0.0.0",
  "description": "test server for luke",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest --colors --verbose --detectOpenHandles",
    "test:watch": "jest --watchAll",
    "coverage": "jest --coverage",
    "coverage:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "start": "nodemon --exec babel-node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luke0408/node-to-nest.git"
  },
  "author": "luke0408",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luke0408/node-to-nest/issues"
  },
  "homepage": "https://github.com/luke0408/node-to-nest#readme",
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "graphql-request": "^7.2.0",
    "http-status-codes": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/node": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2"
  },
  "jest": {
    "testTimeout": 90000,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.js$",
    "collectCoverageFrom": [
      "**/*.js"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testEnvironmentOptions": {
      "NODE_ENV": "test"
    },
    "restoreMocks": true,
    "coveragePathIgnorePatterns": [
      "node_modules",
      "config/*",
      "index.js",
      "test"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "clover",
      "html"
    ]
  }
}
